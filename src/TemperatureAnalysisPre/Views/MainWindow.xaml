        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        Width="1080"
        Height="600"
        prism:ViewModelLocator.AutoWireViewModel="True"
        ShowInTaskbar="True"
        WindowStartupLocation="CenterScreen"
        WindowState="{Binding WindowState}">
